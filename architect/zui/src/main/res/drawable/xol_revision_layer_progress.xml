<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 总进度的颜色 -->
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/color33000000" />
        </shape>
    </item>
    <!-- 缓存的颜色 -->
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/color80ED4D16"
                    android:startColor="@color/colorE6FFA365" />
            </shape>
        </clip>
    </item>
    <!-- 当前进度的颜色 -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:endColor="@color/color80ED4D16"
                    android:startColor="@color/colorE6FFA365" />
            </shape>
        </clip>
    </item>
</layer-list>